@{
    ViewData["Title"] = "Reportes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container p-2">
    <div class="row">
        <div class="col-6">
            <h3 class="text-primary">Lista de Personas</h3>
        </div>        
    </div>

    <div class="pt-3">
        <table id="ReportT" class="table table-striped table-bordered" style="width:100%">
            <tr>
                <td>Id</td>
                <td>Fecha de registro</td>
                <td>Razon Social</td>
                <td>RFC</td>
                <td>Sucursal</td>
                <td>Empleado</td>
                <td>Nombre</td>
                <td>Paterno</td>
                <td>Materno</td>
                <td>IdViaje</td>
            </tr>
        </table>
    </div>

</div>
@section Scripts{
    <script src="~/js/report.js"></script>
}